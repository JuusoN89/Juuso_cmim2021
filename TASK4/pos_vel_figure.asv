function [] = pos_vel_figure(theta, theta_avel, u_vec, v_vec, t)

figure (2)
plot(t, theta)
title(['Piston position 'm/s'])
xlabel('Time (s)')
ylabel('An')
set(gca,'FontSize',12,'FontName','Times New Roman');

xlim([-a-b a+b])
ylim([-a-b a+b])
legend(['Piston speed = ' num2str(v(2)) 'm/s'])



end

